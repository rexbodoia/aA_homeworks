
<h3>Sign up!</h3>
<form action="<%= users_url %>" method="post">
  <input type="hidden" name="authenticity_token" value=" form_authenticity_token">

  <%= render 'shared/form', user: @user %>

    <input type="submit" value="Sign Up!">
</form>

<form action="<%= new_session_url %>" method="get">
  <label for="log_in">Already a member?</label>
    <input type="submit" value="Log in" id="log_in">
</form>
